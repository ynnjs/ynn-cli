	"license": "MIT",
	"dependencies": {
		"cli-style": "0.0.3",
		"commander": "^2.18.0",
		"empty-dir": "^1.0.0",
		"fs-extra": "^7.0.0",
		"inquirer": "^6.2.0",
		"nunjucks": "^3.1.3",
		"ora": "^3.0.0",
		"semver": "^5.5.1"
	},
	"scripts": {
		"demo": "nodemon --exec node test/app/index.js",
